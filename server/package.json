{
  "name": "kevin-marley-cc-2018-server",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/*",
    "server:dev": "cross-env NODE_ENV=development nodemon src/index.js",
    "start": "node src/index.js",
    "test": "npm run lint && npm run test:unit && npm run test:integration --exit",
    "test:integration": "cross-env NODE_ENV=test mocha test/*.integration.js --recursive --sort --timeout 10000 --exit",
    "test:unit": "cross-env NODE_ENV=test nyc mocha test/**/*.test.js --recursive --sort --timeout 10000 --exit",
    "test:watch": "cross-env NODE_ENV=test nodemon --exec \"mocha test/**/*.test.js --recursive\""
  },
  "author": {
    "name": "vapurrmaid",
    "email": "vapurrmaid@gmail.com",
    "url": "https://github.com/vapurrmaid"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "express-rate-limit": "^2.11.0",
    "helmet": "^3.13.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.2.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "node-mocks-http": "^1.7.0",
    "nodemon": "^1.18.3",
    "nyc": "^12.0.2",
    "supertest": "^3.1.0"
  },
  "engines": {},
  "private": true,
  "nyc": {
    "exclude": [
      "index.js",
      "app.js"
    ]
  }
}
